{
  "name": "sveltekit-rate-limiter",
  "version": "0.6.1",
  "author": "Andreas SÃ¶derlund <ciscoheat@gmail.com> (https://blog.encodeart.dev)",
  "description": "A modular rate limiter for SvelteKit. Use in password resets, account registration, etc.",
  "keywords": [
    "sveltekit",
    "rate",
    "limiting",
    "rate limiter",
    "throttle",
    "limiter"
  ],
  "license": "MIT",
  "bugs": "https://github.com/ciscoheat/sveltekit-rate-limiter/issues",
  "repository": {
    "url": "https://github.com/ciscoheat/sveltekit-rate-limiter"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm-run-all -s check build test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "vitest run",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "svelte": "./dist/server/index.js"
    },
    "./server/limiters": {
      "types": "./dist/server/limiters/index.d.ts",
      "svelte": "./dist/server/limiters/index.js"
    },
    "./server/stores": {
      "types": "./dist/server/stores/index.d.ts",
      "svelte": "./dist/server/stores/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@sveltejs/kit": "1.x || 2.x"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-cloudflare": "^3.0.2",
    "@sveltejs/kit": "^2.5.4",
    "@sveltejs/package": "^2.3.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-svelte": "^2.35.1",
    "nanoid": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "publint": "^0.2.7",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.7",
    "tslib": "^2.6.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.4.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "dependencies": {
    "@isaacs/ttlcache": "^1.4.1"
  },
  "type": "module"
}
