{
  "name": "sveltekit-rate-limiter",
  "version": "0.7.0",
  "author": "Andreas SÃ¶derlund <ciscoheat@gmail.com> (https://blog.encodeart.dev)",
  "description": "A modular rate limiter for SvelteKit. Use in password resets, account registration, etc.",
  "keywords": [
    "sveltekit",
    "rate",
    "limiting",
    "rate limiter",
    "throttle",
    "limiter"
  ],
  "license": "MIT",
  "bugs": "https://github.com/ciscoheat/sveltekit-rate-limiter/issues",
  "repository": {
    "url": "https://github.com/ciscoheat/sveltekit-rate-limiter"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm-run-all -s check build test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "vitest run",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "svelte": "./dist/server/index.js"
    },
    "./server/limiters": {
      "types": "./dist/server/limiters/index.d.ts",
      "svelte": "./dist/server/limiters/index.js"
    },
    "./server/stores": {
      "types": "./dist/server/stores/index.d.ts",
      "svelte": "./dist/server/stores/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "server": [
        "./dist/server/index.d.ts"
      ]
    }
  },
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@sveltejs/kit": "1.x || 2.x"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.1",
    "@sveltejs/adapter-cloudflare": "^7.0.3",
    "@sveltejs/kit": "^2.21.1",
    "@sveltejs/package": "^2.3.11",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.9.0",
    "nanoid": "^5.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.12",
    "svelte": "^5.33.12",
    "svelte-check": "^4.2.1",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "dependencies": {
    "@isaacs/ttlcache": "^1.4.1"
  },
  "type": "module"
}
